{
  "name": "my-screeps-code",
  "version": "0.1.0",
  "description": "My code for running Screeps game based on Screeps Typescript Starter",
  "main": "index.js",
  "//": "If you add or change the names of destinations in screeps.json, make sure you update these scripts to reflect the changes",
  "scripts": {
    "lint": "tslint \"src/**/*.ts\"",
    "build": "rollup -c",
    "push-main": "rollup -c --dest main",
    "push-test": "rollup -c --dest test",
    "push-pserver": "rollup -c --dest pserver",
    "push-sim": "rollup -c --dest sim",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch-main": "rollup -cw --dest main",
    "watch-pserver": "rollup -cw --dest pserver",
    "watch-sim": "rollup -cw --dest sim",
    "check": "tslint \"src/**/*.ts\" && rollup -c"
  },
  "author": "",
  "license": "Unlicense",
  "devDependencies": {
    "@types/lodash": "3.10.1",
    "@types/screeps": "2.1.0",
    "@types/source-map": "0.5.7",
    "rollup": "0.53.4",
    "rollup-plugin-clean": "1.0.0",
    "rollup-plugin-commonjs": "8.2.6",
    "rollup-plugin-node-resolve": "3.0.2",
    "rollup-plugin-screeps": "0.1.2",
    "rollup-plugin-typescript2": "0.9.0",
    "tslint": "5.9.1",
    "typescript": "2.6.1"
  },
  "dependencies": {
    "source-map": "0.6.1"
  }
}
